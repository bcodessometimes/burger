<div>
<h1>BURGERS!</h1>
<h4>Menu!!</h4>
 {{#each burgers}}
    <div>
        {{#unless eaten}}
            <p>{{this.burger}}</p>
            <button data-id={{this.id}}
            class="eatBurger">
                NOM NOM</button>
            <hr>
            
            {{/unless}}
        </div>
    {{/each}}

  <h4>EATEN</h4>
    {{#each burgers}}
        <div>
    {{#if eaten}}
    <p>{{this.burger}} </p>
            <button data-id={{this.id}}
            class="deleteBurger">
                DELETE</button>
            <hr>
    {{/if}}
    </div>
  {{/each}}
<hr>
<h4>Add a Burger</h4>
<input type="text" id="addABurger">
<button id="addBurger">Add Burger</button>
</div>
